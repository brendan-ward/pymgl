{
    "version": 8,
    "sources": {
        "geojson": {
            "type": "geojson",
            "data": "file://test.geojson"
        }
    },
    "layers": [
        {
            "id": "box",
            "type": "fill",
            "source": "geojson",
            "paint": {
                "fill-color": "#FF0000",
                "fill-opacity": 0.5
